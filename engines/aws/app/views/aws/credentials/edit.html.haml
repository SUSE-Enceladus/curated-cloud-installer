= page_header(t('engines.aws.credentials.title'))
-# body
= form_with(model: @credential, url: credential_path) do |form|
  %div.form-group
    = form.label :credential_aws_access_key_id, 'AWS Access Key ID'
    = form.text_field :aws_access_key_id, class: 'form-control'
  %div.form-group
    = form.label :credential_aws_secret_access_key, 'AWS Secret Access Key'
    = form.password_field :aws_secret_access_key, class: 'form-control'
  %div.form-group
    = form.submit "Save", class: "btn btn-primary"
-# end
= render('layouts/navigation_buttons') do
  = previous_step_button()
